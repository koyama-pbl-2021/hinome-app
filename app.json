{
  "expo": {
    "name": "HinomeCamera",
    "description": "Album App for travel",
    "owner": "koyama-pbl-2021",
    "slug": "HinomeCamera",
    "privacy": "unlisted",
    "version": "0.1.4",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "koyama-pbl-2021.HinomeCamera",
      "buildNumber": "0.1.4",
      "infoPlist": {
        "NSCameraUsageDescription": "写真をアップロードするためにカメラを使用します",
        "NSPhotoLibraryUsageDescription": "普段カメラを使用しない時間を分析するためにフォトライブラリを使用します"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
